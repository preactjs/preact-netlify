{
  "private": true,
  "name": "blog",
  "description": "Preact's netlify CMS template",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev": "preact watch --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "esm": "^3.2.25",
    "glob": "^7.1.4",
    "jest": "^21.2.1",
    "jest-preset-preact": "^1.0.0",
    "markdown": "^0.5.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.7",
    "preact-render-spy": "^1.2.1",
    "react-syntax-highlighter": "^12.2.1",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "dependencies": {
    "@preact/prerender-data-provider": "^0.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-to-jsx": "^6.10.3",
    "preact": "^10.0.0",
    "preact-cli-plugin-netlify": "^1.5.0",
    "preact-render-to-string": "^5.0.6",
    "preact-router": "^3.0.1",
    "parse-md": "^2.0.4"
  },
  "jest": {
    "preset": "jest-preset-preact"
  },
  "bugs": {
    "url": "https://github.com/preactjs/preact-netlify/issues"
  },
  "homepage": "https://github.com/preactjs/preact-netlify#readme",
  "keywords": [
    "preact",
    "netlify"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preactjs/preact-netlify.git"
  }
}
